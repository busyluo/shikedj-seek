<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity AgentProvince.
    -->
    <changeSet id="20180729023342-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="province_id"
                                 baseTableName="agent_province"
                                 constraintName="fk_agent_province_province_id"
                                 referencedColumnNames="id"
                                 referencedTableName="province"/>

        <addForeignKeyConstraint baseColumnNames="agent_id"
                                 baseTableName="agent_province"
                                 constraintName="fk_agent_province_agent_id"
                                 referencedColumnNames="id"
                                 referencedTableName="agent_admin"/>

    </changeSet>
</databaseChangeLog>
