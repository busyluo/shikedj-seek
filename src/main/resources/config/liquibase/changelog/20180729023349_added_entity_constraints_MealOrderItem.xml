<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity MealOrderItem.
    -->
    <changeSet id="20180729023349-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="dish_id"
                                 baseTableName="meal_order_item"
                                 constraintName="fk_meal_order_item_dish_id"
                                 referencedColumnNames="id"
                                 referencedTableName="dish"/>

        <addForeignKeyConstraint baseColumnNames="order_id"
                                 baseTableName="meal_order_item"
                                 constraintName="fk_meal_order_item_order_id"
                                 referencedColumnNames="id"
                                 referencedTableName="meal_order"/>

    </changeSet>
</databaseChangeLog>
